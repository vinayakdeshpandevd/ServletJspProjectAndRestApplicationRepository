<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>project</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>A</servlet-name>
  <servlet-class>jobportal.ApplyAction</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>A</servlet-name>
  <url-pattern>/apply</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>B</servlet-name>
  <servlet-class>jobportal.ViewAction</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>B</servlet-name>
  <url-pattern>/view</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>C</servlet-name>
  <servlet-class>jobportal.DeleteAction</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>C</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>D</servlet-name>
  <servlet-class>jobportal.EditAction</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>D</servlet-name>
  <url-pattern>/edit</url-pattern>
  </servlet-mapping>
   <servlet>
  <servlet-name>E</servlet-name>
  <servlet-class>jobportal.UpdateValues</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>E</servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>F</servlet-name>
  <servlet-class>jobportal.ViewAllAction</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>F</servlet-name>
  <url-pattern>/viewall</url-pattern>
  </servlet-mapping>
  
</web-app>